#!/bin/bash
echo 'net-tools Installation Script'
sudo apt-get update
sudo apt-get install -y net-tools
which ifconfig
ifconfig

echo 'Telnet Installation script'
sudo apt-get update
sudo apt-get install -y telnet

echo 'Puppet Installation script'
sudo apt-get update
sudo apt-get install puppet
puppet --version
which puppet

echo 'Ansible Installation script'
sudo apt-get update
sudo apt-get install ansible
ansible --version
which ansible
#merging to master
